; Provide the solarized darker palette.

(setq palette-midnight-blue
      '(
        ("base03" . "#080a16")
        ;; background highlight dark
        ("base02" . "#102030")
        ;; emphasized content
        ("base01" . "#889888")
        ;; primary content
        ("base00" . "#456b63")
        ("base0"  . "#b0b0b0")
        ;; comments
        ("base1"  . "#888888")
        ;; background highlight light
        ("base2"  . "#7799dd")
        ;; background light
        ("base3"  . "#99bbff")

        ;;modeline active dark - optional
        ("base4"  . "#334466")
        ;;modeline active light
        ("base04"  . "#667799")
        ;;modeline box
        ("base5"  . "#99AACC")
        ;;modeline box light
        ("base05"  . "#102030")
        ;;modeline foreground dark
        ("base06"  . "#080a16")
        ;;modeline foreground light
        ("base6"  . "#080a16")

        ;; Solarized accented colors
        ("yellow" . "#a09850")
        ("orange" . "#c08133")
        ("red"    . "#a34806")
        ("magenta" . "#d33682")
        ("violet" . "#6c71c4")
        ("blue"   . "#4276ca")
        ("cyan"   . "#23a8c6")
        ("green"  . "#409850")

        ;; Darker and lighter accented colors
        ;; Only use these in exceptional circumstances!
        ("yellow-d" . "#7B6000")
        ("yellow-l" . "#DEB542")
        ("orange-d" . "#8B2C02")
        ("orange-l" . "#F2804F")
        ("red-d"    . "#990A1B")
        ("red-l"    . "#FF6E64")
        ("magenta-d" . "#93115C")
        ("magenta-l" . "#F771AC")
        ("violet-d" . "#3F4D91")
        ("violet-l" . "#9EA0E5")
        ("blue-d"   . "#00629D")
        ("blue-l"   . "#69B7F0")
        ("cyan-d"   . "#00736F")
        ("cyan-l"   . "#69CABF")
        ("green-d"  . "#546E00")
        ("green-l"  . "#B4C342")))


(provide 'palette-midnight-blue)
